    {
      "cell_type": "code",
      "source": [
        "def P(n):\n",
        "  Pn = 1\n",
        "  for i in range(1,n+1):\n",
        "    Pn = Pn * i\n",
        "  return(Pn)\n",
        "print(P(3))\n",
        "def A(k,n):\n",
        "  t = 1\n",
        "  l = 1\n",
        "  for i in range(1,n+1):\n",
        "    t = t * i\n",
        "  for j in range(1,n-k):\n",
        "    l = l * j\n",
        "  Akn = t/l\n",
        "  return(Akn)\n",
        "print(A(4,5))\n",
        "def C(n,k):\n",
        "  t = 1\n",
        "  l = 1\n",
        "  z = 1\n",
        "  for i in range(1,n+1):\n",
        "    t = t * i\n",
        "  for j in range(1,n-k):\n",
        "    l = l * j\n",
        "  for y in range(1,k+1):\n",
        "    z = z * y\n",
        "  Cnk = t/(z*l)\n",
        "  return(Cnk)\n",
        "print(C(13,7))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "1CZXKN6ZPkEL",
